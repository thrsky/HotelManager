<struts>
	
	<package name="action.admin.loginAction" namespace="/admin" extends="struts-default">
		<action name="action.admin.loginadmin" class="com.hotel.action.admin.LoginAdminAction">
			<result name="success">/web/admin/adminindex.html</result>
